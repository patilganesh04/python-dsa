    "def missing_values(arr):\n",
    "\n",
    "    total=0\n",
    "    n=len(arr)\n",
    "    for num in arr:\n",
    "        total+=num\n",
    "    expected_sum=n*(n+1)//2\n",
    "    return expected_sum - total\n",
    "\n",
    "arr=[0, 1, 2, 3, 5]\n",
    "print(missing_values(arr))  # Output: 4"
   ]
